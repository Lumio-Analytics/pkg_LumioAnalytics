{
    "name": "lumioanalytics/lia-php-client",
    "description": "Lumio instegrations api php client",
    "keywords": [
        "lumiotracking",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://lumio-analytics.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "lumio-analytics",
            "homepage": "https://lumio-analytics.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Lumio\\IntegrationAPI\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Lumio\\IntegrationAPI\\" : "test/" }
    }
}
